{
  "manifest_version": 2,

  "name": "Jira extension",
  "description":"The extension can be used to see user's open issues",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "http://*/"

  ]
}
